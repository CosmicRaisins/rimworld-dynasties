<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<ThingDef ParentName="MealCooked">
		<defName>SZ_OrangeChickenMeal</defName>
		<label>orange chicken meal</label>
		<description>An paradoxical ancient chinese cuisine that isn't created by the Chinese people. Quick to cook, taste surprisingly decent.</description>
		<graphicData>
			<texPath>Things/Meals/SZ_OrangeChicken</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>12</MarketValue>
			<WorkToMake>450</WorkToMake>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<tasteThought>AteOrangeChicken</tasteThought>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>

	</ThingDef>

